  "keywords": [
    "pwa"
  ],
  "author": "Cristian Mihaita",
  "license": "MIT",
  "devDependencies": {
    "dotenv": "^8.2.0",
    "http-server": "^0.10.0"
  },
  "dependencies": {
    "azure-storage": "^2.10.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "multiparty": "^4.2.1",
    "ssl-root-cas": "^1.3.1",
    "uuid": "^3.3.3"
  },
  "engines": {
    "node": "10.15.2"
  }
}
