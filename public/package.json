{
  "name": "cris-tasor",
  "version": "1.0.0",
  "description": "A simplistic task management app",
  "main": "index.js",
  "scripts": {
    "start": "npm run start_install && npm run start_local_express",
    "start_local_http": "node -r dotenv/config server-http.js",
    "start_local_express": "node -r dotenv/config server-express.js",
    "start_install": "npm install",
    "start_local": "npm run start_local_express"
